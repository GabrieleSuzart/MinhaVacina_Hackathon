# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 120
trim_trailing_whitespace = true

[*.{json}]
indent_size = 4

[Jenkinsfile]
indent_size = 4

[.eslintrc]
indent_size = 4

[*.md]
trim_trailing_whitespace = false
